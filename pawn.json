{
    "user": "metropolitancityrpg",
    "repo": "samp-timer-fix",
    "include_path": "include",
    "resources": [
        {
            "name": "^timerfix-(.+)-win32.zip$",
            "platform": "windows",
            "archive": true,
            "plugins": ["timerfix.dll"]
        },
        {
            "name": "^timerfix-(.+)-debian10.tar.gz$",
            "platform": "linux",
            "plugins": ["timerfix.so"]
        }
    ],
    "runtime": {
        "plugins": ["metropolitancityrpg/samp-timer-fix"]
    }
}
